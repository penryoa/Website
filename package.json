{
  "name": "personal-react-site-addi",
  "version": "1.0.0",
  "description": "my personal website made with react",
  "scripts": {
    "prebuild": "npm run build:tailwind",
    "build": "react-scripts build",
    "build:tailwind": "postcss tailwindcss -o ./src/assets/custom.css ./src/assets/generated.css",
    "watch:tailwind": "postcss -w src/assets/custom.css -o src/assets/generated.css",
    "start": "set PORT=3001 && react-scripts start",
    "dev": "run-p watch:tailwind start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penryoa/Website.git"
  },
  "author": "Addi Penry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/penryoa/Website/issues"
  },
  "homepage": "https://github.com/penryoa/Website#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^10.4.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "postcss": "^8.4.12",
    "prettier": "^2.1.2",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "final-form": "^4.18.7",
    "hamburger-react": "^2.4.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.3.5",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "text-mask-addons": "^3.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
